{
	"name": "fakesharper",
	"publisher": "fakesharper",
	"displayName": "fakesharper",
	"description": "ReSharper features for Visual Studio Code",
	"version": "1.0.0",
	"preview": false,
	"icon": "assets/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/fakesharper/fakesharper"
	},
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Formatters",
		"Linters"
	],
	"keywords": [
		"C#",
		"csharp",
		".NET",
		".NET Core",
		"ASP.NET",
		"dotnet",
		"resharper"
	],
	"activationEvents": [
		"workspaceContains:**/*.sln",
		"onCommand:fakesharper.inspectcode",
		"onCommand:fakesharper.cleandiagnostics",
		"onCommand:fakesharper.cleanalldiagnostics",
		"onCommand:fakesharper.cleanupcode",
		"onCommand:fakesharper.cleanupCurrentFile",
		"onCommand:fakesharper.inspectCurrentFile",
		"onCommand:fakesharper.reloaddiagnostics"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fakesharper.inspectcode",
				"title": "Inspect Project Code",
				"category": "fakesharper"
			},
			{
				"command": "fakesharper.cleandiagnostics",
				"title": "Clean Diagnostics",
				"category": "fakesharper"
			},
			{
				"command": "fakesharper.cleanalldiagnostics",
				"title": "Clean All Diagnostics",
				"category": "fakesharper"
			},
			{
				"command": "fakesharper.cleanupcode",
				"title": "Cleanup Project Code",
				"category": "fakesharper"
			},
			{
				"command": "fakesharper.cleanupCurrentFile",
				"title": "Cleanup Current File",
				"category": "fakesharper"
			},
			{
				"command": "fakesharper.inspectCurrentFile",
				"title": "Inspect Current File",
				"category": "fakesharper"
			},
			{
				"command": "fakesharper.reloaddiagnostics",
				"title": "Reload Diagnostics",
				"category": "fakesharper"
			},
			{
				"command": "fakesharper.dupfinder.run",
				"title": "Run Dupfinder",
				"category": "fakesharper"
			},
			{
				"command": "fakesharper.dupfinder.clean",
				"title": "Clean Duplicates",
				"category": "fakesharper"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "fakesharper",
					"title": "fakesharper",
					"icon": "resources/icon/csharp.svg"
				}
			]
		},
		"views": {
			"fakesharper": [
				{
					"id": "dupfinder",
					"name": "Dupfinder"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "fakesharper.dupfinder.run",
					"when": "view == dupfinder",
					"group": "dupfinder"
				},
				{
					"command": "fakesharper.dupfinder.clean",
					"when": "view == dupfinder",
					"group": "dupfinder"
				},
				{
					"command": "fakesharper.cleanupcode",
					"when": "view == dupfinder",
					"group": "cleanupcode"
				},
				{
					"command": "fakesharper.inspectcode",
					"when": "view == dupfinder",
					"group": "inspectcode"
				},
				{
					"command": "fakesharper.cleanalldiagnostics",
					"when": "view == dupfinder",
					"group": "inspectcode"
				}
			],
			"editor/context": [
				{
					"when": "resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.cleanupCurrentFile",
					"group": "2_fakesharper@1"
				},
				{
					"when": "resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.inspectCurrentFile",
					"group": "2_fakesharper@2"
				},
				{
					"when": "resourceExtname == .csproj || resourceExtname == .sln || resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.cleanalldiagnostics",
					"group": "2_fakesharper@7"
				},
				{
					"when": "resourceExtname == .csproj || resourceExtname == .sln || resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.reloaddiagnostics",
					"group": "2_fakesharper@9"
				}
			],
			"explorer/context": [
				{
					"when": "resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.cleanupCurrentFile",
					"group": "2_fakesharper@1"
				},
				{
					"when": "resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.inspectCurrentFile",
					"group": "2_fakesharper@2"
				},
				{
					"when": "resourceExtname == .csproj || resourceExtname == .sln || resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.cleanupcode",
					"group": "2_fakesharper@3"
				},
				{
					"when": "resourceExtname == .csproj || resourceExtname == .sln || resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.inspectcode",
					"group": "2_fakesharper@4"
				},
				{
					"when": "resourceExtname == .csproj || resourceExtname == .sln || resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.dupfinder.run",
					"group": "2_fakesharper@5"
				},
				{
					"when": "resourceExtname == .csproj || resourceExtname == .sln || resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.dupfinder.clean",
					"group": "2_fakesharper@6"
				},
				{
					"when": "resourceExtname == .csproj || resourceExtname == .sln || resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.cleanalldiagnostics",
					"group": "2_fakesharper@7"
				},
				{
					"when": "resourceExtname == .csproj || resourceExtname == .sln || resourceExtname == .cs || resourceExtname == .fs",
					"command": "fakesharper.reloaddiagnostics",
					"group": "2_fakesharper@9"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "fakesharper",
			"properties": {
				"fakesharper.cleanupOnSave": {
					"type": "boolean",
					"default": false,
					"description": "When enabled will cleanup code on save."
				},
				"fakesharper.inspectcodeOnSave": {
					"type": "boolean",
					"default": false,
					"description": "When enabled will cleanup code on save."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.31.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"fast-xml-parser": "^3.16.0"
	},
	"__metadata": {
		"id": "8c7b3c7a-9422-436f-9572-cc012e144877",
		"publisherDisplayName": "fakesharper",
		"publisherId": "0f641873-9305-4157-8f0b-36886fce4719"
	}
}